<?xml version="1.0" encoding="UTF-8"?>

        <?import javafx.scene.control.Button?>
        <?import javafx.scene.control.TableColumn?>
        <?import javafx.scene.control.TableView?>
        <?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.MenuController">
    <children>
        <TableView fx:id="tableView" layoutY="32.0" prefHeight="310.0" prefWidth="600.0" editable="true">
            <columns>
                <TableColumn fx:id="idColumn" prefWidth="33.0" text="id" />
                <TableColumn fx:id="nameColumn" prefWidth="144.0" text="name" />
                <TableColumn fx:id="descriptionColumn" prefWidth="212.0" text="description" />
                <TableColumn fx:id="ingredientsColumn" prefWidth="155.0" text="ingredients" />
                <TableColumn fx:id="priceColumn" prefWidth="37.0" text="price" />
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
        </TableView>
        <Button fx:id="backButton" layoutX="280.0" layoutY="353.0" mnemonicParsing="false" onAction="#switchToMenu" text="Back" />
    </children>
</AnchorPane>